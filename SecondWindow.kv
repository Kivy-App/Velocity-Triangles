						self.parent.y - self.parent.height/2 + float(root.y0Text)/2 + float(root.yLtText)/2)\
						if float(root.xLtText)>float(root.x0tText)\
						else\
						(self.parent.x - self.parent.width/2 + float(root.xLtText) - dp(40),\
						self.parent.y - self.parent.height/2 + float(root.y0Text)/2 + float(root.yLtText)/2)

				Label:
					text: '\u0394V\u03B8'
