                    cols:2
                    size_hint_y: 0.1
                    BoxLayout:
                        size_hint_x:0.3
                        padding:dp(10)
                        spacing:dp(10)
                        MDRectangleFlatIconButton:
                            icon:'arrow-left-bold'
                            md_bg_color:[0.4,.4,.4,1]
