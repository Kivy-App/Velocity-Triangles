<MidScreen>:
    name: 'mid_sc'
    id: mid
    BoxLayout:
        orientation: 'vertical'
        size_hint: 1,1
        MDGridLayout:
            cols:4
            size_hint_y: 0.08
            md_bg_color:0.18,0.18,0.18,1

            MDBoxLayout:
                size_hint:0.15,1
                MDIconButton:
                    id:info_button
                    user_font_size:dp(30)
                    icon:'menu'
                    pos:self.parent.pos
                    size:self.parent.size
                    theme_text_color: 'Custom'
                    text_color: 1,1,1,1
                    on_release: nav_drawer.set_state('open')

            MDFloatLayout:
                size_hint: 0.17,1
                orientation: 'horizontal'
                canvas:
                    Color:
                        rgba: 1,1,1,1
                    Line:
                        width: dp(1)
                        points: [self.x+self.width-dp(10),self.y+dp(10),self.x+self.width-dp(10),self.y+self.height-dp(10)]
                Image:
                    source: 'images/logos/icon_orange4.png'
                    size_hint:0.8,0.8
                    pos_hint: {'x':-0.08,'y':0.1}


            MDBoxLayout:
                size_hint: 0.68,1
                Label:
                    size_hint: 1,1
                    text: 'VTA-APP'
                    color: [1,1,1,1]
                    font_name: 'BlackHanSans-Regular.ttf'
                    text_size: self.parent.size
                    halign: 'left'
                    valign: 'middle'
                    font_size: dp(28)
        GridLayout:
            rows:2
            size_hint_y: 0.06
			##### Buttons ####
            GridLayout:
                size_hint_y:0.98
                cols: 4
                MDFlatButton:
                    text: 'HUB'
                    on_press:
                        root.manager.current = 'hub_sc'
                        sv_mid.scroll_y = 1
                MDFlatButton:
                    text: 'MID'
                    on_press:
                        sv_mid.scroll_y = 1
                MDFlatButton:
                    text: 'TIP'
                    on_press:
                        root.manager.current = 'tip_sc'
                        sv_mid.scroll_y = 1
                MDFlatButton:
                    text: 'Comp'
                    on_press:
                        root.manager.current = 'comp_sc'
                        sv_mid.scroll_y = 1

            BoxLayout:
                size_hint_y: 0.02
                orientation: 'horizontal'
                canvas:
                    Color:
                        rgba: 0.2,0.2,0.2,1
                    Line:
                        width: dp(1.5)
                        points: (self.x + dp(2)+self.width/4,self.y+dp(2.5),self.x+dp(2)+self.width/2,self.y+dp(2.5))

        ScrollView:
            size: root.height, root.width
            size_hint: 1,0.86
            scroll_wheel_distance: '150dp'
            effect_cls: 'ScrollEffect'
            id: sv_mid

