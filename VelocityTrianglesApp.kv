            canvas:
                Color:
                    rgba: 0.6862,0.9333,0.9333,0.737
                Rectangle:
                    pos: self.pos
                    size: self.size

                Color:
                    rgba:0.5862,0.9333,0.9333,0.737
                Rectangle:
                    pos:self.x+20, self.y
                    size:self.width-40, self.height-20

                Color:
                    rgba:0,0,0,1
                Line:
                    #:set x0 dp(120)
                    #:set y0 dp(150)
                    #:set x1 dp(280)
                    #:set y1 dp(150)
                    #:set U  x1-x0
                    width:1.5
                    points: (x0,y0,x1,y1) ### U ###
                Line:
                    width:1.5
                    points: (x0,y0,x1-U*float(root.rnText)-U*float(root.pText)/2,y0+U*float(root.fText))
                Line:
                    width:1.5
                    points: (x1-U*float(root.rnText)-U*float(root.pText)/2,y0+U*float(root.fText),x1,y1)
                Line:
                    width:1.5
                    points: (x1,y1,x1-U*float(root.rnText)+U*float(root.pText)/2,y0+U*float(root.fText))
                Line:
                    width:1.5
                    points: (x0,y0,x1-U*float(root.rnText)+U*float(root.pText)/2,y0+U*float(root.fText))

                Color:
                    rgba:0,0,0,1
                Line:
                    width: 1.
                    rectangle: (self.x+20, self.y, self.width-40, self.height-20)



