                    pos:self.x+20, self.y
                    size:self.width-40, self.height-20

                Color:
                    rgba:0,0,0,1
                Line:
                    width: 1.
                    rectangle: (self.x+20, self.y, self.width-40, self.height-20)

###################    Color of the Lines    ##################
                Color:
                    rgba:0,0,0,1
###################    U line    ##########################
                Line:
                    width:1.5
                    points: (dp(root.x0Text),dp(root.y0Text),dp(root.x1Text),dp(root.y1Text)) 

###################    V1 Line    ##########################
                Line:
                    width:1.5
                    points: (dp(root.x0Text),dp(root.y0Text),dp(root.xRText),dp(root.yRText))

###################    W1 Line    ##########################
                Line:
                    width:1.5
                    points: (dp(root.x1Text),dp(root.y1Text),dp(root.xRText),dp(root.yRText))

###################    W2 Line    ##########################
                Line:
                    width:1.5
                    points: (dp(root.x1Text),dp(root.y1Text),dp(root.xLText),dp(root.yLText))

###################    V2 Line    ##########################
                Line:
                    width:1.5
                    points: (dp(root.x0Text),dp(root.y0Text),dp(root.xLText),dp(root.yLText))

########### Lines, Dash lines and Labels ###################
###################    U dashLine    ##########################
                Line:
                    width: 1
                    points: (dp(root.x0Text),dp(root.y0Text)-dp(20),dp(root.x1Text),dp(root.y1Text)-dp(20))

###################    U Dash lines    ##########################
                Line:
                    width: 1
                    points: (dp(root.x0Text),dp(root.y0Text),dp(root.x0Text),dp(root.y0Text)-dp(30))
                    dash_length: 5
                    dash_offset: 5
                Line:
                    width: 1
                    points: (dp(root.x1Text),dp(root.y0Text),dp(root.x1Text),dp(root.y0Text)-dp(30))
                    dash_length: 5
                    dash_offset: 5

###################    Vx Line    ##########################
                Line:
                    width: 1
                    points:
                        (dp(root.x1Text) + dp(20),dp(root.y0Text),dp(root.x1Text) + dp(20),dp(root.yRText))\
                        if dp(root.xRText) < dp(root.x1Text) else\
                        (dp(root.xRText)+ dp(20),dp(root.y0Text),dp(root.xRText)+ dp(20),dp(root.yRText))

###################    Vx Dash lines    ##########################
                Line:
                    width: 1
                    points:
                        (dp(root.x1Text),dp(root.y1Text),dp(root.x1Text) + dp(30),dp(root.y0Text))\
