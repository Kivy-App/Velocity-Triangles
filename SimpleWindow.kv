						size: self.width - dp(20),self.height - dp(20)
						pos: self.x + dp(10),self.y + dp(10)
                    Color:
                        rgba: 0,0,0,1
                    Line:
                        width: dp(1.5)
                        rectangle: (self.x + dp(10),self.y + dp(10),self.width - dp(20),self.height - dp(20))




			BoxLayout:
