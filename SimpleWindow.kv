					Line:
						width: dp(1)
						points: (float(root.x1Text),self.y + float(root.y0Text),float(root.x1Text),self.y + float(root.y0Text)-dp(30))
						dash_length: dp(5)
						dash_offset: dp(5)

	###################    Vx Line    ##########################
					Line:
						width: dp(1)
						points:
							(float(root.x1Text) + dp(20),self.y + float(root.y0Text),float(root.x1Text) + dp(20),self.y + float(root.yRText))\
							if float(root.xRText) < float(root.x1Text) else\
							(float(root.xRText)+ dp(20),self.y + float(root.y0Text),float(root.xRText)+ dp(20),self.y + float(root.yRText))

	###################    Vx Dash lines    ##########################
					Line:
						width: dp(1)
						points:
							(float(root.x1Text),self.y + float(root.y1Text),float(root.x1Text) + dp(30),self.y + float(root.y0Text))\
							if float(root.xRText) < float(root.x1Text) else\
							(float(root.x1Text),self.y + float(root.y0Text),float(root.xRText) + dp(30) ,self.y + float(root.y1Text))
						dash_length: dp(5)
						dash_offset: dp(5)
					Line:
						width: dp(1)
						points:
							(float(root.xRText),self.y + float(root.yRText),float(root.x1Text) + dp(30),self.y + float(root.yRText))\
							if float(root.xRText) < float(root.x1Text)  else\
							(float(root.xRText),self.y + float(root.yRText),float(root.xRText)+dp(30),self.y + float(root.yRText))
						dash_length: dp(5)
						dash_offset: dp(5)

	###################   ΔVθ   Line    ##########################
					Line:
						width: dp(1)
						points: (float(root.xLText),self.y + float(root.yLText) + dp(20),float(root.xRText),self.y + float(root.yRText) + dp(20))

	###################    ΔVθ  Dash lines    ##########################
					Line:
						width: dp(1)
						points: (float(root.xLText),self.y + float(root.yLText),float(root.xLText),self.y + float(root.yLText)+dp(30))
						dash_length: dp(5)
						dash_offset: dp(5)
					Line:
						width: dp(1)
						points: (float(root.xRText),self.y + float(root.yRText),float(root.xRText),self.y + float(root.yRText)+dp(30))
						dash_length: dp(5)
						dash_offset: dp(5)

	################### Circles of stages numbers ###############################
					Line:
						width: dp(1.)
						circle:
							(float(root.xLText) - dp(15),self.y + float(root.yLText) + dp(17),dp(10))

					Line:
						width: dp(1.)
						circle:
							(float(root.xRText) + dp(15),self.y + float(root.yLText) + dp(17),dp(10))



				 ################################  Arrows   ##########################################
	################### Base Velocity Arrows   ##########################

	                ################## U Arrow ########################
	                Color:
                        rgba: (1,1,1,0) if float(root.x1Text) - float(root.x0Text) < 30 else (0,0,0,1)
					Triangle:
						points:float(root.x0Text),self.y + float(root.y0Text),float(root.x0Text)+dp(25),self.y + float(root.y0Text)+dp(5),float(root.x0Text)+dp(25),self.y + float(root.y0Text)-dp(5)

                    ################## The other 4 Base Arrows ########################
                    Color:
                        rgba: (0,0,0,1)
					Triangle:
						points:float(root.x0Text),self.y + float(root.y0Text),float(root.xL1uText),self.y + float(root.yL1uText),float(root.xL1dText),self.y + float(root.yL1dText)

					Triangle:
						points:float(root.x0Text),self.y + float(root.y0Text),float(root.xL2uText),self.y + float(root.yL2uText),float(root.xL2dText),self.y + float(root.yL2dText)

					Triangle:
						points:float(root.x1Text),self.y + float(root.y1Text),float(root.xR1uText),self.y + float(root.yR1uText),float(root.xR1dText),self.y + float(root.yR1dText)

					Triangle:
						points:float(root.x1Text),self.y + float(root.y1Text),float(root.xR2uText),self.y + float(root.yR2uText),float(root.xR2dText),self.y + float(root.yR2dText)

	##################  U Line Arrows  ####################
	                Color:
                        rgba: (1,1,1,0) if float(root.x1Text) - float(root.x0Text) < 40 else (0,0,0,1)
					Triangle:
						points:
							float(root.x0Text),self.y + float(root.y0Text)-dp(20),float(root.x0Text)+dp(20),\
							self.y + float(root.y0Text)-dp(20)+dp(3),float(root.x0Text)+dp(20),self.y + float(root.y0Text)-dp(20)-dp(3)
					Triangle:
						points:
							float(root.x1Text),self.y + float(root.y1Text)-dp(20),float(root.x1Text)-dp(20),\
							self.y + float(root.y1Text)-dp(20)+dp(3),float(root.x1Text)-dp(20),self.y + float(root.y1Text)-dp(20)-dp(3)

	##################  ΔVθ Line Arrows  ####################
                    Color:
                        rgba: (1,1,1,0) if float(root.xRText) - float(root.xLText) < 25 else (0,0,0,1)
					Triangle:
						points:
							(float(root.xLText),self.y + float(root.yLText) + dp(20),float(root.xLText)+ dp(20),self.y + float(root.yLText) + dp(20) + dp(3),\
							float(root.xLText)+ dp(20),self.y + float(root.yLText) + dp(20) - dp(3))\
							if root.tc_namemText == 'Compressor Middle' else\
							(float(root.xRText),self.y + float(root.yRText) + dp(20),float(root.xRText)- dp(20),self.y + float(root.yRText) + dp(20) + dp(3),\
							float(root.xRText)- dp(20),self.y + float(root.yRText) + dp(20) - dp(3))

	#####################  Vx Line Arrows  ####################
	                Color:
	                    rgba: (1,1,1,0) if (float(root.yRText) - float(root.y1Text)) < 25 else (0,0,0,1)
					Triangle:
						points:
							(float(root.x1Text) + dp(20),self.y + float(root.y1Text),float(root.x1Text) + dp(20) + dp(3),\
							self.y + float(root.y1Text)+dp(20),float(root.x1Text) + dp(20) - dp(3),self.y + float(root.y1Text)+dp(20))\
							if float(root.xRText) < float(root.x1Text) else\
							(float(root.xRText)+ dp(20),self.y + float(root.y1Text),float(root.xRText)+ dp(20) + dp(3),\
							self.y + float(root.y1Text) + dp(20),float(root.xRText)+ dp(20) - dp(3),self.y + float(root.y1Text) + dp(20))

	###################   Labels   ##########################
#				Label:
#					pos: (float(root.ptnm_xText),self.parent.y - self.parent.height/2 + dp(30))
#					text: root.tc_namemText
#					color: 1,0,0,1

				Label:
					text: 'U'
					pos: self.parent.x, self.parent.y - self.parent.height/2 +dp(45)

				Label:
					text: 'Vx'
					pos:
						(self.parent.x + float(root.UText)/2 + dp(35),\
						self.parent.y - self.parent.height/2  + float(root.y0Text) + float(root.UText)*float(root.fText)/2)\
						if float(root.x1Text) > float(root.xRText) else\
						(self.parent.x - self.parent.width/2 + float(root.xRText) + dp(40),\
						self.parent.y - self.parent.height/2  + float(root.y0Text) + float(root.UText)*float(root.fText)/2)

				Label:
					text: '\u0394V\u03B8'
					pos:
						-float(root.UText)/2-float(root.x0Text)+float(root.xLText)+float(root.UText)*float(root.pText)/2,\
						self.parent.y - self.parent.height/2 + float(root.y0Text) + float(root.UText)*float(root.fText) + dp(35)

				Label:
					text:'1' if root.tc_namemText == 'Compressor Middle' else '3'
					pos:
						-float(root.UText)/2-float(root.x0Text)+float(root.xLText) - dp(15),\
						self.parent.y - self.parent.height/2 + float(root.y0Text) + float(root.UText)*float(root.fText) + dp(17)

				Label:
					text:'2'
					pos:
						-float(root.UText)/2-float(root.x0Text)+float(root.xRText) + dp(15),\
						self.parent.y - self.parent.height/2 + float(root.y0Text) + float(root.UText)*float(root.fText) + dp(17)

            GridLayout:
				cols:2
				size_hint_y: 0.1
				canvas:
					Color:
						rgba: 0.6862,0.97,0.97,0.6
					Rectangle:
						pos: self.pos
						size: self.size
#                size_hint_y: 0.15
				BoxLayout:
					size_hint_x:0.3
					padding:dp(10)
					spacing:dp(10)
					MDRectangleFlatIconButton:
						icon:'arrow-left-bold'
						md_bg_color:[0.4,.4,.4,1]
						text_color: [1,1,1,1]
						size_hint:0.25,1
						text:"Back"
						on_release:
							root.manager.current = "Main"
							root.manager.transition.direction = "right"

				BoxLayout:
					size_hint_x:0.7
