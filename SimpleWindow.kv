<SimWindow>:
	name: "simple"

	ScrollView:
		size: root.height, root.width
		size_hint: 1,1
		scroll_wheel_distance: '40dp'

		GridLayout:
			rows: 7
			size_hint: 1,1.71

            GridLayout:
                cols:2
                size_hint_y: 0.08
                BoxLayout:
                    size_hint_x:0.15
                    MDIconButton:
                        id:info_button
                        pos:self.parent.pos
                        size:self.parent.size
                        user_font_size:dp(30)
                        icon:'menu'
                        on_release: root.nav_drawer.set_state('open')

                BoxLayout:
                    size_hint_x: 0.85
                    canvas:
                        Color:
                            rgba:0,0,0,0
                        Rectangle:
                            pos: self.pos
                            size: self.size
                        Color:
                            rgba: 1,1,1,1
                        Line:
                            width: dp(1)
                            points: [self.x,self.y+dp(10),self.x,self.y+self.height-dp(10)]
                    Label:
                        text: '  VTA-APP'
                        color: [1,1,1,1]
                        font_name: 'BlackHanSans-Regular.ttf'
                        text_size: self.size
                        halign: 'left'
                        valign: 'middle'
                        font_size: dp(30)
############ First Title ####################
			BoxLayout:
				size_hint_y: 0.08
				canvas:
					Color:
						rgba: 0.066,0.392,0.505,0.768
					Rectangle:
						pos: self.pos
						size: self.size

				Label:
					text: 'Velocity Analysis'
					font_size: dp(25)
					bold: True

